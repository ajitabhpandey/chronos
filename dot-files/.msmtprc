# ~/.msmtprc
# ajitabhpandey@ajitabhpandey.info

account domain2
host smtp.domain2.com
port 587
protocol smtp
from user1@domain2.com
user user1@domain2.com
password "password"
tls on
tls_nocertcheck
#
account domain1
host smtp.domain1.com
port 587
protocol smtp
from user1@domain1.com
user user1@domain1.com
password "password"
tls on
tls_nocertcheck
#
account gmail
host smtp.gmail.com
port 587
protocol smtp
auth on
from user1@gmail.com
user user1@gmail.com
password "password"
tls on
tls_nocertcheck

account default: gmail

